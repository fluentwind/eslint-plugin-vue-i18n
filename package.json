{
  "name": "@fluentwind/eslint-plugin-vue-i18n",
  "version": "0.0.8",
  "description": "Lint vue i18n",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "vue-i18n"
  ],
  "author": "fluentwind",
  "main": "lib/index.js",
  "homepage": "https://github.com/fluentwind/eslint-plugin-vue-i18n",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluentwind/eslint-plugin-vue-i18n.git"
  },
  "scripts": {
    "test": "mocha tests/lib --recursive",
    "lint-no-duplicated-values": "eslint tests/data --rulesdir lib/rules --rule {'no-duplicated-values':['warn']}",
    "lint-no-undefined-key": "eslint tests/data --rulesdir lib/rules --rule {'no-undefined-key':['warn']}"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.1.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
